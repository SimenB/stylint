{
  "name": "stylint",
  "version": "1.5.9",
  "description": "A linter for stylus",
  "author": "Ross Patton",
  "license": "GPL-3.0",
  "keywords": [
    "cli",
    "css",
    "hinter",
    "lint",
    "linter",
    "styl",
    "style",
    "stylint",
    "stylus",
    "stylus-hint",
    "stylus-lint"
  ],
  "main": "index.js",
  "files": [
    "bin/",
    "index.js",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SimenB/stylint"
  },
  "bugs": {
    "url": "https://github.com/SimenB/stylint/issues"
  },
  "bin": {
    "stylint": "./bin/stylint"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.14.1",
    "istanbul": "0.4.5",
    "jsdoc": "3.4.3",
    "mocha": "3.2.0",
    "sinon": "1.17.7",
    "touch": "1.0.0"
  },
  "dependencies": {
    "async": "2.1.4",
    "chalk": "1.1.3",
    "chokidar": "1.6.1",
    "columnify": "1.5.4",
    "glob": "7.1.1",
    "lodash.defaults": "4.2.0",
    "path-is-absolute": "1.0.1",
    "stampit": "3.1.2",
    "strip-json-comments": "2.0.1",
    "user-home": "2.0.0",
    "yargs": "6.6.0"
  },
  "scripts": {
    "cover": "istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/test.js",
    "docs": "jsdoc src/ --recurse --destination docs/",
    "lint": "eslint .",
    "precover": "npm run lint",
    "pretest": "npm run lint",
    "start": "node --harmony index.js",
    "test": "mocha"
  }
}
