    "eslint": "6.0.1",
    "jsdoc": "3.6.2",
    "mocha": "6.1.4",
    "nyc": "^14.1.1",
    "sinon": "7.3.2",
    "touch": "3.1.0"
  },
  "dependencies": {
    "async": "3.1.0",
    "chalk": "2.4.2",
    "chokidar": "3.0.1",
    "columnify": "1.5.4",
    "glob": "7.1.4",
    "lodash.defaults": "4.2.0",
    "path-is-absolute": "2.0.0",
    "stampit": "4.2.0",
    "strip-json-comments": "3.0.1",
    "user-home": "2.0.0",
    "yargs": "13.2.4"
  },
  "scripts": {
